#include <iostream>
#include <string>
#include <array>
#include <list>

int main() {
    // Declaraci칩n de un arreglo est치tico con 5 elementos
    std::array<int, 5> datos;

    // Lista de nombres inicializada con algunos valores
    std::list<std::string> personas = {"Ana", "Luis", "Marta"};

    // Asignaci칩n a la primera posici칩n del arreglo
    datos.at(0) = 10;

    // Mostrar el segundo nombre de la lista
    auto it = personas.begin();
    std::advance(it, 1); // Avanza al segundo elemento
    std::cout << *it << std::endl;

    return 0;
}
